  target 'ContactsAppTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'IQKeyboardManagerSwift'
    pod 'Alamofire'
    pod 'SDWebImage'

  end

  target 'ContactsAppUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'IQKeyboardManagerSwift'
    pod 'Alamofire'
    pod 'SDWebImage'

  end

end